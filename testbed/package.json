{
	"name": "testbed",
	"version": "0.0.1",
	"private": false,
	"description": "testbed",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./client/out/extension.js",
	"scripts": {
		"clean": "tsc -b --clean",
		"compile": "tsc -b",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "tsc -b -w"
	},
	"contributes": {
		"commands": [
			{
				"command": "testbed.openFile",
				"title": "Open Test File"
			}
		],
		"configuration": {
			"properties": {
				"testbed.enable": {
					"default": true,
					"description": "Control whether eslint is enabled for JavaScript files or not.",
					"type": "boolean"
				},
				"testbed.options": {
					"default": {},
					"description": "The eslint options object to provide args to the eslint command.",
					"type": "object"
				}
			},
			"title": "TestBed configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"enabledApiProposals": []
}
