{
	"name": "testbed",
	"description": "testbed",
	"main": "./client/out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"clean": "tsc -b --clean",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "testbed.openFile",
				"title": "Open Test File"
			}
		],
		"configuration": {
			"properties": {
				"testbed.enable": {
					"default": true,
					"description": "Control whether eslint is enabled for JavaScript files or not.",
					"type": "boolean"
				},
				"testbed.options": {
					"default": {},
					"description": "The eslint options object to provide args to the eslint command.",
					"type": "object"
				}
			},
			"title": "TestBed configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@playform/build": "0.0.7"
	},
	"enabledApiProposals": []
}
