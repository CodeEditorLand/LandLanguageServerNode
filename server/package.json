{
	"name": "vscode-languageserver",
	"version": "0.0.1",
	"private": false,
	"description": "Language server implementation for node",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./lib/node/main.js",
	"browser": {
		"./lib/node/main.js": "./lib/browser/main.js"
	},
	"typings": "./lib/common/api.d.ts",
	"bin": {
		"installServerIntoExtension": "./bin/installServerIntoExtension"
	},
	"scripts": {
		"all": "npm run clean && npm run compile && npm run lint && npm test",
		"all:publish": "git clean -xfd . && npm install && npm run compile:publish && npm run lint && npm test",
		"clean": "node ../node_modules/rimraf/dist/esm/bin.mjs lib",
		"compile": "node ../build/bin/tsc -b ./tsconfig.json",
		"compile:publish": "node ../build/bin/tsc -b ./tsconfig.publish.json",
		"prepack": "npm run all:publish",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"preversion": "npm test",
		"watch": "node ../build/bin/tsc -b ./tsconfig.watch.json -w"
	},
	"dependencies": {
		"vscode-languageserver-protocol": "3.17.6-next.3"
	},
	"devDependencies": {
		"typescript-esbuild": "0.4.5",
		"vscode-languageserver-textdocument": "1.0.11"
	},
	"publishConfig": {
		"access": "public"
	}
}
